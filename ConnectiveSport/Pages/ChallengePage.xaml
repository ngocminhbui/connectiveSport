<?xml version="1.0" encoding="UTF-8"?>
<local:ChallengePageXaml xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="ConnectiveSport.ChallengePage"
	BackgroundColor="{StaticResource grayLight}"
	xmlns:local="clr-namespace:ConnectiveSport">
	
	<local:ChallengePageXaml.Content>


		<Picker IsVisible="false" IsEnabled="false" x:Name="sportPicker">

			<Picker.Items>
            <x:String>1</x:String> 
            <x:String>2</x:String>
       		</Picker.Items>
			
		</Picker>

	</local:ChallengePageXaml.Content>

	<local:ChallengePageXaml.ToolbarItems>
		<ToolbarItem x:Name="btnJoin" Icon="ic_add_white" AutomationId="joinLeagueButton" />
	</local:ChallengePageXaml.ToolbarItems>

	
</local:ChallengePageXaml>